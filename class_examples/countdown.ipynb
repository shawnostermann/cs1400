    "def countdown(n):\n",
    "    if n <= 0:               # This is a base case\n",
    "        print ('Blastoff!')  # We donâ€™t recurse for the base case\n",
    "    else:\n",
    "        print (n)\n",
    "        countdown(n-1)                # Recurse\n",
    "\n",
    "countdown(10)"
   ]
